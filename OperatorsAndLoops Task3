using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Task3
{
    class Program
    {
        static void Main(string[] args)
        {
            //Применяем цикл for так как нам известны чёткие границы последовательности
            int minValue = 5;
            int maxValue = 96;
            int summationVar = 7;

            for (minValue = 5; minValue <= maxValue; minValue+=summationVar)
            {
                Console.WriteLine(minValue);
            }

            Console.ReadLine();
        }
    }
}
