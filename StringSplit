using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Task2
{
    class Program
    {
        static void Main(string[] args)
        {
            string text = "Дана строка с текстом";
            string[] divide = text.Split(' ');

            foreach (var item in divide)
            {
                Console.WriteLine(item);
            }
            Console.ReadLine();
        }
    }
}
